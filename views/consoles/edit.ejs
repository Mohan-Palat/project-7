<body><h1>Edit Console</h1>
    <form action="/consoles/<%= console._id%>?_method=PUT" method="POST"> <!--Method is the http verb that we are sending-->
        <label for="name">Name</label>
        <input type="text" name="name" id="name" value="<%= console.name %>" /><br/>
        
        <label for="image">Image</label>
        <input type="text" name="image" id="image" value="<%=console.image%>" /><br/>
        
        <input type="submit" value="Update Console Info">
      </form>

     </body>