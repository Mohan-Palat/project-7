<%- include ("../partials/search.ejs")%>
<div class="pure-g">
<div class="pure-u-1-2 1-box"><h1>Consoles</h1></div>
<% if(isAuthenticated){ %>
<div class="pure-u-1-2 1-box"><a href="/consoles/new">Create a New Console</a></div><%}%> 
</div>

<div class="pure-g">
    <% consoles.forEach(console => { %>
    <div class="pure-u-1 pure-u-sm-1-4">
       <a href="/consoles/<%=console._id%>"><img class="pure-img" src="<%=console.image%>">
        <%=console.name%></a></div>
    <%})%>
</div>





